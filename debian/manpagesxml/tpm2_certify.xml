      <segtitle>Diagnostic</segtitle>
      <seglistitem>
        <seg><errorcode>0</errorcode></seg>
        <seg>Program exited successfully.</seg>
      </seglistitem>
      <seglistitem>
        <seg><errorcode>non-zero</errorcode></seg>
        <seg>Program exited with some error.</seg>
      </seglistitem>
    </segmentedlist>
  </refsect1>
